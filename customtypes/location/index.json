{
  "id": "location",
  "label": "Location",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "config": {
          "label": "Name",
          "placeholder": "Enter city, state..."
        },
        "type": "Text"
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 320
          },
          "thumbnails": [
            {
              "name": "xs",
              "width": 320,
              "height": null
            },
            {
              "name": "sm",
              "width": 640,
              "height": null
            },
            {
              "name": "md",
              "width": 480,
              "height": null
            },
            {
              "name": "lg",
              "width": 640,
              "height": null
            },
            {
              "name": "xl",
              "width": 720,
              "height": null
            },
            {
              "name": "xxl",
              "width": 960,
              "height": null
            }
          ]
        }
      },
      "startDateTime": {
        "type": "Timestamp",
        "config": {
          "label": "Start Date Time",
          "placeholder": ""
        }
      },
      "endDateTime": {
        "type": "Timestamp",
        "config": {
          "label": "End Date Time",
          "placeholder": ""
        }
      },
      "venue": {
        "type": "Group",
        "config": {
          "label": "Venue",
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "Name",
                "placeholder": "Enter venue name..."
              }
            },
            "address": {
              "type": "StructuredText",
              "config": {
                "label": "Address",
                "placeholder": "Enter venue address...",
                "allowTargetBlank": false,
                "multi": "paragraph"
              }
            },
            "placeId": {
              "type": "Text",
              "config": {
                "label": "Place Id",
                "placeholder": "Enter venue Google PlaceID..."
              }
            },
            "description": {
              "type": "StructuredText",
              "config": {
                "allowTargetBlank": true,
                "placeholder": "Enter venue description...",
                "label": "Description",
                "multi": "paragraph,strong,em,hyperlink",
                "labels": ["button-outline", "button-fill"]
              }
            }
          }
        }
      }
    },
    "SEO": {
      "uid": {
        "config": {
          "label": "Uid",
          "placeholder": ""
        },
        "type": "UID"
      },
      "metaTitle": {
        "config": {
          "label": "Meta Title",
          "placeholder": ""
        },
        "type": "Text"
      },
      "metaDescription": {
        "config": {
          "label": "Meta Description",
          "placeholder": ""
        },
        "type": "Text"
      },
      "socialCard": {
        "config": {
          "label": "Social Card",
          "fields": {
            "socialCardImage": {
              "config": {
                "label": "Social Card Image",
                "constraint": {},
                "thumbnails": []
              },
              "type": "Image"
            },
            "socialCardTitle": {
              "config": {
                "label": "Social Card Title",
                "placeholder": ""
              },
              "type": "Text"
            },
            "socialCardDescription": {
              "config": {
                "label": "Social Card Description",
                "placeholder": ""
              },
              "type": "Text"
            }
          }
        },
        "type": "Group"
      }
    }
  }
